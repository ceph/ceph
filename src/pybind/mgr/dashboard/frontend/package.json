    "table": "6.8.0",
    "transifex-i18ntool": "1.1.0",
    "ts-node": "9.0.0",
    "typescript": "5.4.5"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/common"
  }
}
